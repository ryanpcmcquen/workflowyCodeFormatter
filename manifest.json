{
    "manifest_version": 2,

    "name": "WorkFlowy code formatter",
    "description": "Makes backtick wrapped text look like code! {openSource}",
    "version": "0.7.0",

    "icons": {
        "128": "wcf_icon.png"
    },

    "content_scripts": [
        {
            "run_at": "document_end",
            "matches": ["https://workflowy.com/*"],

            "js": [
                "assets/codeFormatter/codeFormatter.js",
                "workflowyCodeFormatter.js"
            ]
        }
    ]
}
